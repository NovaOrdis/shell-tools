#!/usr/bin/env bash
#
# JSON/bash utilities
#

JSON_SHLIB_VERSION=1.0.0

#
# Convert a space separated list "a b c" to a JSON list ["a", "b", "c"], which is returned to stdout
#
function space-separated-list-to-json-list() {

    debug "${FUNCNAME[0]}($@)"

    local space_separated_list=$1

    [[ -z ${space_separated_list} ]] && { echo "[]"; return 0; }

}